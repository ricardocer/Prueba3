   "source": [
    "from random import random\n",
    "\n",
    "N = 10**6\n",
    "n = 0\n",
    "\n",
    "for i in range(N):\n",
    "    x = random()\n",
    "    y = random()\n",
    "    if x**2 + y**2 < 1:\n",
    "        n += 1\n",
    "    else: \n",
    "        n += 0\n",
    "    \n",
    "print(\"La aproximación es: \",4*(n/N))"
   ]
  }
